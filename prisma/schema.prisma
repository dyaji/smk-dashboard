generator client {
  provider   = "prisma-client-js"
  engineType = "binary"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model DailyMetric {
  id        String   @id @default(cuid())
  date      DateTime @unique
  support   Int
  undecided Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
